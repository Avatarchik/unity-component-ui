function render(props)
	return (
		<Panel x={0} y={0} width={300} height={300}>
			<Button x={90} y={10} width={50} height={50} onClick={props.onPlusClicked}/>
			<Button x={150} y={10} width={50} height={50} onClick={props.onMinusClicked}/>
			<Text x={50} y={80} width={200} height={50} text={props.count}/>
		</Panel>
	)
end